#1.1 Tipo NaN (Not a Number)

import math
from decimal import Decimal

#NaN (Not a number)
a = float('NaN')
print(f'a: {a}')
print(math.isnan(a)) #True

#Modulo math
a = float('nan')
print(f'Es de tipo nan: {math.isnan(a)}') #True

#Modulo decimal
a = Decimal('nan')
print(f'Es de tipo nan: {math.isnan(a)}') #True
